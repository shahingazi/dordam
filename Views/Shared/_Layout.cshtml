<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="price comparision site by DorDam.com">

    <title>DorDam.com</title>

    <link rel="shortcut icon" type="image/x-icon" href="images/favicon.ico">

    <!-- jQuery -->
    <script src="~/lib/jquery/jquery.js"></script>

    <!-- Bootstrap4 files-->
    <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.js"></script>
    <link href="~/lib/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />

    <!-- Font awesome 5 -->
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />

    <!-- plugin: fancybox  -->
    <script src="~/lib/fancybox/jquery.fancybox.js"></script>
    <link href="~/lib/fancybox/jquery.fancybox.css" rel="stylesheet" />

    <!-- plugin: owl carousel  -->
    <link href="~/lib/OwlCarousel2/assets/owl.carousel.css" rel="stylesheet" />
    <script src="~/lib/OwlCarousel2/owl.carousel.js"></script>
    <link href="~/lib/OwlCarousel2/assets/owl.theme.default.css" rel="stylesheet" />

    <!-- custom style -->
    <link href="~/css/ui.css" rel="stylesheet" />
    <link href="~/css/responsive.css" rel="stylesheet" media="only screen and (max-width: 1200px)" />

    <!-- custom javascript -->
    <script src="~/js/script.js"></script>

</head>
<body>
    <header class="section-header">
        <section class="header-main">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-3">
                        <div class="brand-wrap">
                            <img class="logo" src="~/images/logo-dark.png">
                            <h2 class="logo-text">DorDam.com</h2>
                        </div>
                    </div>
                    <div class="col-lg-6 col-sm-6">
                        <partial name="_SearchPartial" />
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <partial name="_LoginPartial" />
                    </div>
                </div>
            </div>
        </section>
        <section>
            <partial name="_NavPartial.cshtml" />
        </section>
    </header>
    <partial name="_CookieConsentPartial" />
    <section class="section-content padding-y-sm bg">
        <div class="container">
            @RenderBody()
        </div>
    </section>

    <footer class="section-footer bg-secondary">
        <partial name="_FooterPartial.cshtml" />
    </footer>

    @RenderSection("Scripts", required: false)
</body>
</html>

































