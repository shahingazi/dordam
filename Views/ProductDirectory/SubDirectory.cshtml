@using PriceComparisonWeb.ViewModel
@model List<SubDirectory>

<div class="row">
    @foreach (var category in Model)
    {
        <div class="col-lg-3">
            <h4>@category.Name</h4>
            <ul class="">
                @foreach (var subcategory in category.Items)
                {
                    <li>
                        <a href=""> @subcategory.Name </a>
                    </li>
                }
            </ul>

        </div>
    }

</div>